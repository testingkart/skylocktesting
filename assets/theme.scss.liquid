$slideshowSpeed: 0.3s;

.home-slideshow-wrapper {
  height: 600px;
  overflow: hidden;
  position: relative;
  @include transition(height $slideshowSpeed $easeOutQuad);

  &.content-width {
    max-width: $maxWidth - $gutter*2;
    padding: 0 $gutter;
    margin: $gutter*2 auto;
  }

  @include breakpoint(s) {
    height: 400px;
    margin-bottom: $gutter*2;

    .template-product-slideshow & {
      margin-bottom: 0;
    }

    + .contrast {
      margin-top: $gutter*2;
    }

    + .default {
      margin-top: $gutter*2;

      &:before {
        content: '';
        display: block;
        margin: 0 30px;
        height: 1px;
        background: $borderColor;
      }
    }
  }

  * {
    -webkit-tap-highlight-color: rgba(0,0,0,0);
  }

}

.home-slideshow-loader {
  @extend .add-to-cart-loader;
  left: 50%;
  right: auto;
  z-index: 1;

  .slides-ready & {
    display: none;
  }
}

.home-slideshow {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0;
  z-index: 2;
  @include transition(opacity $slideshowSpeed $easeOutQuad);

  .slides-ready & {
    opacity: 1;
  }
}

.home-slideshow-navigation {
  width: 70px;
  height: 100%;
  position: absolute;
  z-index: 5;
  top: 0;
  text-align: center;
  font-size: 32px;
  cursor: pointer;
  visibility: hidden;
  color: $slideTextColor;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  @include transition(all $slideshowSpeed $easeOutQuad);

  .slides-ready & {
    visibility: visible;
  }

  .template-product-slideshow & {
    color: $productSlideshowColor;
  }
}

.home-slideshow-previous {
  left: 0;
}

.home-slideshow-next {
  right: 0;
}

.home-slideshow-pagination {
  position: absolute;
  bottom: 0;
  z-index: 5;
  text-align: center;
  left: 50%;
  line-height: 50px;
  font-size: 0;
  visibility: hidden;
  color: $slideTextColor;
  @include transform(translateX(-50%));
  @include transition(color $slideshowSpeed $easeOutQuad);

  .slides-ready & {
    visibility: visible;
  }

  .template-product-slideshow & {
    color: $productSlideshowColor;
    width: 100%;
  }

  > span {
    width: 20px;
    height: 20px;
    padding: 5px;
    display: inline-block;
    cursor: pointer;
  }

  span span {
    width: 10px;
    height: 10px;
    display: block;
    background: transparent;
    border: 1px solid $slideTextColor;
    border-radius: 5px;

    .template-product-slideshow & {
      border-color: $productSlideshowColor;
    }
  }

  .active span {
    background: $slideTextColor;

    .template-product-slideshow & {
      background: $productSlideshowColor;
    }
  }
}

.home-slide {
  position: absolute;
  width: 100%;
  background-color: $bodyBackgroundColor;

  .opening-screen & {
    background-size: cover;
    background-repeat: no-repeat;
    background-position: center center;

    @include breakpoint(s) {
      background: none !important; // override inline styles
    }
  }
}

.transition-fade .home-slide {
  opacity: 0;

  &.active {
    opacity: 1;
    z-index: 1;
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(opacity $slideshowSpeed $easeOutQuad);
  }

  &.next.left,
  &.prev.right {
    opacity: 1;
  }

  &.active.left {
    opacity: 0;
  }
  &.active.right {
    opacity: 0;
  }
}

.transition-slide .home-slide {
  visibility: hidden;

  &.active {
    visibility: visible;
    z-index: 1;
    @include transform(translateX(0));
  }

  &.next {
    @include transform(translateX(100%));
  }

  &.prev {
    @include transform(translateX(-100%));
  }

  &.next.left,
  &.prev.right,
  &.active.left,
  &.active.right {
    @include transition(transform $slideshowSpeed $easeOutQuad);
  }

  &.next.left,
  &.prev.right {
    @include transform(translateX(0));
    visibility: visible;
    z-index: 10;
  }

  &.active.left {
    @include transform(translateX(-100%));
  }
  &.active.right {
    @include transform(translateX(100%));
  }
}

.slide-image {
  width: 100%;
  display: block;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;

  @include breakpoint(s) {
    margin-bottom: $gutter;
  }

  .opening-screen & {
    display: none;

    @include breakpoint(s) {
      display: block;
    }
  }
}
  
.slide-image-overlay {
    background-color: rgba(63, 63, 63, 0.6);
    display: block;
    height: 100%;
    position: absolute;
    top: 0;
    width: 100%;
}

.slide-text {
  position: absolute;
  width: 100%;
  max-width: 1200px;
  top: 45%;
  left: 50%;
  .preOrderText {
    color: #fff;
    font-weight: normal;
    margin-top: 10px;
  }
  .textEnd {
    color: #fff;
    display: block;
    font-size: 18px;
    font-weight: bold;
    margin-top: 12px;
  }	
  @include transform(translate(-50%,-50%));

  @include breakpoint(s) {
    width: 100% !important;
    top: 5% !important;
    text-align: center !important;
    left: 0 !important;
    margin: 0 auto !important;
    right: 0 !important;
    padding: 0 10px !important;
    @include transform(translate(0,0) !important);
  }

  .full-bleed &, .opening-screen & {
    padding-top: 60px;
  }

  .text-aligned-center & {
    text-align: center;
    padding: 60px 0 0;
    max-width: 500px;
  }

  .text-aligned-left & {
    padding-right: 30%;
    padding-left: $gutter;

    @include lt-ie9 {
      padding-left: 0;
      left: 100px;
    }

    @media (max-width: 1370px) {
      padding-left: 0;
      left: 100px;
      @include transform(translate(0,-50%));
    }

    .content-width & {
      padding-left: 0;
      left: 100px;
      @include transform(translate(0,-50%));
    }
  }

  .text-aligned-right & {
    padding-left: 30%;
    padding-right: $gutter;
    text-align: right;

    @include lt-ie9 {
      padding-right: 0;
      right: 100px;
      left: auto;
    }

    @media (max-width: 1370px) {
      padding-right: 0;
      right: 100px;
      left: auto;
      @include transform(translate(0,-50%));
    }

    .content-width & {
      padding-right: 0;
      right: 100px;
      left: auto;
      @include transform(translate(0,-50%));
    }
  }

  h2, h3, p {
    margin: 20px 0;
    color: $slideTextColor;
  }

  &.slide-text-use-shadows {
    h2, h3, p {
      text-shadow: 0px 2px 7px rgba(0,0,0,0.15);
    }
  }

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }
}

.slide-text h2, .slide-text h3 {
  font-size: 32px;
  font-family: proxima_novasemibold;
  margin-top: 25px;
  margin-bottom: 0;
}

.slide-text h2 {
  font-weight: normal;
}

.slide-text h3 {
  font-weight: normal;
}

.slide-text p {
    font-size: 20px;
    line-height: 30px;
