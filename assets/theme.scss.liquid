    border-bottom: none;
    cursor: pointer;

    &:hover { font-weight: bold; }

    &.active {
      border-color: $borderColor;
      height: 42px;
      border-radius: 2px 2px 0 0;
      font-weight: bold;
    }
  }
}

.rte .tabs-content {
  display: block;
  width: 100%;
  height: auto;
  position: relative;
  list-style: none;
  padding: 0;
  margin: 0 0 30px;
  overflow: hidden;

  > li {
    display: none;
    margin: 30px 0 20px;

    &.active { display: block; }
  }
}

.breadcrumbs {
  text-align: center;
