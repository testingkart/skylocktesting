.header-tools .navigation {
  display: inline-block;
  padding: 0 $gutter 0 0;
  text-align: left;
  position: relative;

  .collapsed-navigation & {
    display: none;
  }

  @include breakpoint(s) {
    display: none;
  }

  ul {
    margin: 0;
    padding: 0;
  }

  li {
    display: inline;
    position: relative;
    padding-bottom: 20px;
    line-height: 120px;
  }

  a {
    color: $headerNavigationColor;
    padding: 13px 20px;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        color: #fff;
      }

      .header-bleed-dark-colors & {
        color: #000;
      }
    }

    &:hover {
      color: $headerNavigationColorHover;

      @media (min-width: 721px) {
        .header-bleed-light-colors & {
          color: #fff;
        }

        .header-bleed-dark-colors & {
          color: #000;
        }
      }
    }
  }

  .no-touch & li:hover ul {
    display: block;
  }

  .touch & li.open ul {
    display: block;
  }

  ul ul {
    display: none;
    position: absolute;
    width: 160px;
    left: -80px;
    margin-left: 50%;
    text-align: center;
    font-size: 14px;
    border-radius: 5px;
    top: 100%;

    &:before, &:after {
      bottom: 100%;
      left: 50%;
      border: solid transparent;
      content: " ";
      height: 0;
      width: 0;
      position: absolute;
      pointer-events: none;
      border-color: rgba(136, 183, 213, 0);
    }

    &:before {
      border-bottom-color: $headerNavigationDropdownBorder;
      border-width: 8px;
      margin-left: -8px;

      @media (min-width: 721px) {
        .header-bleed-light-colors & {
          border-bottom-color: #fff;
        }

        .header-bleed-dark-colors & {
          border-bottom-color: #000;
        }
      }
    }

    &:after {
      border-bottom-color: $headerNavigationDropdownBackground;
      border-width: 7px;
      margin-left: -7px;

      @media (min-width: 721px) {
        .header-bleed-light-colors & {
          border-bottom-color: #fff;
        }

        .header-bleed-dark-colors & {
          border-bottom-color: #000;
        }
      }
    }
  }

  ul ul {
    background: $headerNavigationDropdownBackground;
    border: 1px solid $headerNavigationDropdownBorder;
    padding: 13px 0;

    @media (min-width: 721px) {
      .header-bleed-light-colors & {
        background: #fff;
        border: 1px solid #fff;
      }

      .header-bleed-dark-colors & {
        background: #000;
        border: 1px solid #000;
      }
    }

    li {
      display: block;
      padding-bottom: 0;
      line-height: 1.5;
      margin: 5px 0;
    }

    a {
      color: $headerNavigationDropdownColor;
      display: block;
      padding: 10px;

      @media (min-width: 721px) {
        .header-bleed-light-colors & {
          color: #000;
        }

        .header-bleed-dark-colors & {
          color: #fff;
        }
      }

      &:hover {
        color: $headerNavigationDropdownColorHover;

        @media (min-width: 721px) {
          .header-bleed-light-colors & {
            color: #000;
          }

          .header-bleed-dark-colors & {
            color: #fff;
          }
        }
      }
    }
  }
}

