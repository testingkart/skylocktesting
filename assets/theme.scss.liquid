    text-decoration: line-through;
    opacity: 0.4;
    margin-left: 7px;
  }
}

.template-index {

  .main-content {
    width: 100%;
    max-width: 100%;
    /*padding: 0;*/
  }

  .sticky-header ~ .main-content {
    padding-top: 0;

    @include breakpoint(s) {
      padding-top: 0;
    }
  }

  &.navigation-below-header .sticky-header ~ .main-content {
    padding-top: 0;

    @include breakpoint(s) {
      padding-top: 0;
    }
  }

  .sticky-header.full-bleed-slideshow ~ .main-content,
  .sticky-header.lower-than-slideshow ~ .main-content {
    padding-top: 0;
  }


  .section-title {
    margin-bottom: $gutter*2;
  }
}

.home-module-wrapper {

  &.contrast {
    background: $contrastBackgroundColor;
    background-image: $contrastBackgroundImage;

    @include retina {
      background-image: $contrastBackgroundImage2x;
      background-size: $contrastBackgroundImageSize;
    }
  }
}

.home-module {
  max-width: 1135px;
  margin: 0 auto;
  padding: $gutter/2 $gutter 85px;

  .contrast & {
    color: $contrastColor;

    a {
      color: $contrastAccentColor;

      &:hover {
        color: $contrastAccentColorHover;
      }
    }

    h1, h2, h3, h4, h5, h6 {
      color: $contrastHeadingColor;
    }

    .section-title {
      color: $contrastHeadingColor;
    }

    .meta {
      color: $contrastMetaColor;
    }

    .button, .submit, button, input[type="submit"], input[type="button"] {
      background: $contrastAccentColor;
      color: $contrastPrimaryButtonColor;

      &:hover {
        background: $contrastAccentColorHover;
        color: $contrastPrimaryButtonColor;
      }

      &.disabled, &.disabled:hover {
        background: $contrastDisabledButtonBackground;
        color: $contrastDisabledButtonColor;
      }
    }

    input[type="text"], input[type="email"], input[type="password"], input[type="search"], textarea {
      border: 1px solid $contrastFormTextInputBorder;
      background: $contrastFormTextInputBackground;
      color: $contrastFormTextInputColor;

      @include placeholder {
        color: $contrastFormTextInputColor;
        opacity: 0.7;
      }

      &.error {
        border: 1px solid $contrastErrorColor;
        color: $contrastErrorColor;

        @include placeholder {
          color: $contrastErrorColor;
          opacity: 0.7;
        }
      }
    }

    .select-wrapper {
      background-color: $contrastFormSelectBackground;
      border: 1px solid $contrastFormSelectBorder;
      color: $contrastFormSelectColor;

      &:after {
        color: $contrastFormSelectColor;
      }
    }

    .error-message {
      color: $contrastErrorColor;
    }
  }
}

.default + .default:before {
  content: '';
  display: block;
  margin: 0 $gutter;
  height: 1px;
  background: $borderColor;
