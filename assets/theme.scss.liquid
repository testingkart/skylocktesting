.home-hero-text, .home-hero-image {
  font-size: 16px;
  display: inline-block;
  vertical-align: middle;

  @include breakpoint(s) {
    width: 100%;
    display: block;
    text-align: center;
  }
}

.home-hero-text {
  	left: 0;
    margin: 0 auto;
    padding-right: 0;
    position: absolute;
    right: 0;
    text-align: center;
    width: 32.292%;
  	top: 420px;
  h2, h3, p {
    margin: 20px 0;
    color: $slideTextColor;
  }

  &.home-hero-text-use-shadows {
    h2, h3, p {
      text-shadow: 0px 2px 7px rgba(0,0,0,0.15);
    }
  }

  h2, h3 {
    font-size: 30px;
    margin-bottom: 10px;
    @include breakpoint(m) {
      font-size: 42px;
    }

    @include breakpoint(m) {
      font-size: 32px;
    }
  }

  p {
    font-family: proxima_novalight;
    font-size: 20px;
    font-weight: 100;
    line-height: 30px;
    margin: 0 auto;
    
    @include breakpoint(m) {
      font-size: 26px;
    }

    @include breakpoint(m) {
      font-size: 20px;
    }
  }

  a {
    border: none;
    margin-top: 10px;
    color:#488cce;
    
  }

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }

  @include breakpoint(s) {
    @include order(2);
    padding: 0;
  }
}

.home-hero-image {
  	padding: 0;
  	width: 100%;
	background: rgba(0, 0, 0, 0) url("{{ 'home-hero-product-image.png' | asset_url }}") repeat scroll center top / cover ;
    height: 840px;
  img {
  	width: 100%;
  }
  @include breakpoint(s) {
    @include order(1);
    padding: 0;
    margin-bottom: $gutter;
    text-align: center;
  }
}

.home-full-width-video.video-highlight {

  + .no-content {
    display: none;
  }

  .fluid-width-video-wrapper {
    margin: 0;
  }
}

.home-full-width-feature {
  text-align: center;
  padding-top: 95px;

  @include breakpoint(s) {
    padding-top: 60px;
    padding-bottom: 60px;
  }

  h2 {
    font-size: 40px;
	font-weight: normal;
    @include breakpoint(s) {
