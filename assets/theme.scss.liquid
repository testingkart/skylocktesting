    margin: 15px 0 0 0;
  }
}

.template-collection {

  .page-title {
    margin-bottom: 0;
  }

  .pagination {
    max-width: 100%;
  }
}

.main-content.collection-has-featured-image {
  max-width: 100%;
  padding: 0;

  .no-touch .sticky-header ~ & {
    padding-top: 120px;
  }

  .no-touch .navigation-below-header .sticky-header ~ & {
    padding-top: 170px;
  }

  .breadcrumbs {
    margin-top: 80px;
  }
}

.collection-header-wrapper {
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center center;
  min-height: 500px;
  padding: 50px 30px 60px;
  position: relative;
  text-align: center;
  display: table;
  width: 100%; // required for Firefox

  &:before {
    content: '';
    position: absolute;
    top: 0; right: 0;
    bottom: 0; left: 0;
    background: rgba(0,0,0,0.15);
    z-index: 1;
  }

  @mixin ie8mobile {
    background: none!important; // override inline style
    height: auto;
    min-height: 0;
    padding: 0 $gutter;

    &:before {
      content: none;
    }
  }

  @include lt-ie9 {
    @include ie8mobile;
  }

  @include breakpoint(s) {
    @include ie8mobile;
  }
}

.collection-header {
  display: table-cell;
  vertical-align: middle;
  position: relative;
  z-index: 2;
  width: 100%;

  @mixin ie8mobile {
    display: block;
  }

  @include lt-ie9 {
    @include ie8mobile;
  }

  @include breakpoint(s) {
    @include ie8mobile;
  }

  h1, .collection-description {
    color: white;
    text-shadow: 0px 1px 4px rgba(0,0,0,0.15);

    @mixin ie8mobile {
      color: inherit;
      text-shadow: none;
    }

    @include lt-ie9 {
      @include ie8mobile;
    }

    @include breakpoint(s) {
      @include ie8mobile;
    }
  }

  h1 {
    margin-top: 0;

    @mixin ie8mobile {
      margin-top: 60px;
      color: $headingColor;
    }

    @include lt-ie9 {
      @include ie8mobile;
    }

    @include breakpoint(s) {
      @include ie8mobile;
    }
  }

  .collection-description {
    font-family: $bodyFont;
    font-size: 20px;
    font-weight: normal;
    -webkit-font-smoothing: initial;

    -webkit-font-smoothing: #{'{% if settings.body-font == "Bitter, serif" %}'} antialiased #{'{% endif %}'};
    -webkit-font-smoothing: #{'{% if settings.body-font == "Roboto, sans-serif" %}'} antialiased #{'{% endif %}'};
    -webkit-font-smoothing: #{'{% if settings.body-font == "\'Open Sans\', sans-serif" %}'} antialiased #{'{% endif %}'};
    -webkit-font-smoothing: #{'{% if settings.body-font == "Volkhov, serif" %}'} antialiased #{'{% endif %}'};
    -webkit-font-smoothing: #{'{% if settings.body-font == "Lato, sans-serif" %}'} antialiased #{'{% endif %}'};

    @mixin ie8mobile {
      color: $bodyColor;
      font-size: 16px;
    }

    @include lt-ie9 {
      @include ie8mobile;
    }

    @include breakpoint(s) {
      @include ie8mobile;
    }
  }
}

.collection-description {
  margin-top: $gutter*1.5;
}

.collection-tag-selector {
  text-align: center;
  margin-top: $gutter*1.5;

  .select-wrapper {
    display: inline-block;
    width: auto;
    margin: 0;
  }
}

.collection-products {
  font-size: 0;
