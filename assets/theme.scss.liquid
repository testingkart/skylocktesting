  -webkit-font-smoothing: #{'{% if settings.meta-font == "Roboto, sans-serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.meta-font == "\'Open Sans\', sans-serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.meta-font == "Volkhov, serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.body-font == "Lato, sans-serif" %}'} antialiased #{'{% endif %}'};

  font-size:      #{' {% if settings.meta-font-small-caps %} '}12px #{' {% endif %} '};
  letter-spacing: #{' {% if settings.meta-font-small-caps %} '}0.1em #{' {% endif %} '};
  text-transform: #{' {% if settings.meta-font-small-caps %} '}uppercase #{' {% endif %} '};
}

/* Basic text */
img {
  max-width: 100%;
}

ul, ol, dl {
  padding: 0;
  margin: 23px 0;
}

ul {
  list-style: outside none;
}

ol {
  list-style: outside decimal;
}

li {
  margin: 12px 0;
}

hr {
  height: 2px;
  background: $borderColor;
  border: none;
}

/* Tables */

table {
  background: transparent;
  color: $bodyColor;
  border: 1px solid $borderColor;
  border-top: none;
  font-size: 14px;
  border-radius: 5px;
  border-collapse: separate;
  margin: $gutter 0;

  &.clean {
    border: none;
    border-radius: 0;
    font-size: 16px;
  }

  .column-title {
    display: none;
  }
}

tr:first-child td:first-child, tr:first-child th:first-child {
  border-top-left-radius: 5px;

  thead ~ tbody & {
    border-radius: 0;
  }
}

tr:first-child td:last-child {
  border-top-right-radius: 5px;

  thead ~ tbody & {
    border-radius: 0;
  }
}

tr:last-child td:first-child, tr:last-child th:first-child {
  border-bottom-left-radius: 5px;
}

tr:last-child td:last-child {
  border-bottom-right-radius: 5px;
}

thead th:first-child {
  border-top-left-radius: 5px;
}
thead th:last-child {
  border-top-right-radius: 5px;
}

thead {
  background: $tableHeaderBackground;

  .clean & {
    background: none;
  }
}

th {
  background: $tableHeaderBackground;
  color: $tableHeaderColor;
  font-weight: bold;

  .clean & {
    background: none;
    color: $headingColor;
  }
}

tr td, tr th {
  border-top: 1px solid $borderColor;
  padding: $gutter/(3/2);

  .clean & {
    border: none;
  }
}

tr td {
  border-left: 1px solid $borderColor;

  &:first-child {
    border-left: none;
  }
}

table.mobile-layout {
  border-bottom: none;

  .column-title {
    font-weight: 700;
    width: 114px;
    padding-right: 23px;
    display: inline-block;
  }

  thead {
    display: none;
  }

  tr td {
    width: 100%;
    display: block;
    text-align: left;
    border-top: 1px solid $borderColor !important;
    border-left: none;

    /* < IE 10 fix */
    float:left;
    clear:left;
  }

  tr:first-child td:first-child {
    border-radius: 5px 5px 0 0 !important;
  }

  td:last-child {
    border-bottom: none;
  }

  tr:last-child td:last-child {
    border-bottom: 1px solid $borderColor !important;
    border-radius: 0 0 5px 5px !important;
  }

  tr:nth-child(even) td {
    background: $tableAltBackground;
  }
}

/* Buttons */

.button, .submit, button, input[type="submit"], input[type="button"] {
  background: $accentColor;
  font-family: $primaryButtonFontFamily;
  font-weight: $primaryButtonFontWeight;
  color: $primaryButtonColor;
  -webkit-appearance: none;
  padding: 0 $gutter;
  font-size: 16px;
  border-radius: 5px;
  height: 50px;
  line-height: 50px;
  display: inline-block;
  cursor: pointer;
  border: none;

  &:hover {
    background: $accentColorHover;
    color: $primaryButtonColor;
  }

  font-size:      #{' {% if settings.primary-button-font-small-caps %} '}14px #{' {% endif %} '};
  letter-spacing: #{' {% if settings.primary-button-font-small-caps %} '}0.1em #{' {% endif %} '};
  text-transform: #{' {% if settings.primary-button-font-small-caps %} '}uppercase #{' {% endif %} '};

  &.secondary {
    background: $secondaryButtonBackground;
    color: $secondaryButtonColor;

    &:hover {
      background: $secondaryButtonBackgroundHover;
      color: $secondaryButtonColor;
    }
  }

  &.disabled {
    background: $disabledButtonBackground;
    color: $disabledButtonColor;
    opacity: 1; /* fix for iOS */
  }
}

/* Forms & buttons */

form {
  font-size: 14px;
}

label {
  font-size: 16px;

  .inline-input-wrapper & {
    font-weight: normal;
    font-size: 14px;
    margin-left: 5px;
    display: inline-block;
    vertical-align: middle;
  }
}

.input-wrapper {
  margin: $gutter 0;
}

.inline-input-wrapper {
  margin: $gutter/3 0;
}

input, textarea {
  border-radius:0;

  label ~ & {
    margin-top: $gutter/3;
  }

  &:focus {
    outline: none;
  }
}

input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="telephone"],
input[type="tel"],
input[type="number"],
textarea {
  -webkit-appearance: none;
  border: 1px solid $borderColor;
  color: $bodyColor;
  display: block;
  width: 100%;
  border-radius: 5px;
  -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;

  @include placeholder {
    color: $bodyColor;
    opacity: 0.7;
  }

  &.error {
    color: $errorColor;
    border: 1px solid $errorColor;

    @include placeholder {
      color: $errorColor;
      opacity: 0.7;
    }
  }
}

.error-message, .errors {
  color: $errorColor;
}

input[type="text"],
input[type="email"],
input[type="password"],
input[type="search"],
input[type="telephone"],
input[type="tel"],
input[type="number"] {
  height: 50px;
  padding: 15px;

  @include lt-ie9 {
    padding: 0 15px;
    line-height: 50px;
  }
}

input[type="password"] {
  @include lt-ie9 {
    font-family: Arial, sans-serif;
  }
}

textarea {
  padding: $gutter/2;
  max-width: 100%;
}

input[type=submit] {
  cursor: pointer;
}

.select-wrapper {
  background-color: $formSelectBackground;
  cursor: pointer;
  border-radius: 5px;
  border: 1px solid $formSelectBorder;
  color: $formSelectColor;
  padding: 13px $gutter*1.5 13px $gutter/2;
  position: relative;
  text-align: left;
  width: 100%;
  font-size: 14px;
  margin: $gutter 0;

  &:after {
    color: $formSelectColor;
    content: "\E010";
    margin-top: -8px;
    position: absolute;
    right: $gutter/2;
    top: 50%;
    z-index: 1;
    @include iconFont();
  }

  select {
    color: black;
    opacity: 0;
    filter: alpha(opacity=0); /* IE8 */
    position: absolute;
    top: 0; right: 0;
    bottom: 0; left: 0;
    cursor: pointer;
    z-index: 2;
    width: 100%; /* needed for Firefox */
    height: 100%; /* needed for IE */

    -webkit-appearance: none;
  }

}

/* iframes and embeds */

iframe {
  border: none;
}

/* RTE areas */

.rte {

  word-break: break-word;

  @extend .clearfix;

  > *:first-child {
    margin-top: 0;
  }

  > *:last-child {
    margin-bottom: 0;
  }

  @include all-elems {
    margin-left: auto;
    margin-right: auto;
    max-width: 640px;

    &.highlight {
      max-width: 100%;
    }
  }

  ul {
    list-style: disc;
  }

  ul, ol, dl {
    padding-left: $gutter;
  }

  blockquote {
    font-size: 20px;

    > *:first-child {
      margin-top: 0;
    }

    > *:last-child {
      margin-bottom: 0;
    }

    &.highlight {
      color: $headingColor;
      font-family: $headingFont;
      font-size: 24px;
      -webkit-font-smoothing: initial;

      -webkit-font-smoothing: #{'{% if settings.heading-font == "Bitter, serif" %}'} antialiased #{'{% endif %}'};
      -webkit-font-smoothing: #{'{% if settings.heading-font == "Roboto, sans-serif" %}'} antialiased #{'{% endif %}'};
      -webkit-font-smoothing: #{'{% if settings.heading-font == "\'Open Sans\', sans-serif" %}'} antialiased #{'{% endif %}'};
      -webkit-font-smoothing: #{'{% if settings.heading-font == "Volkhov, serif" %}'} antialiased #{'{% endif %}'};
      -webkit-font-smoothing: #{'{% if settings.body-font == "Lato, sans-serif" %}'} antialiased #{'{% endif %}'};
    }
  }

  cite {
    font-style: normal;
    font-size: 16px;
    color: $metaColor;
  }

}

.fluid-width-video-wrapper {
  width:100%;
  position:relative;
  padding:0;
  margin: $gutter 0;

  iframe, object, embed {
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
  }
}

/* apply a natural box layout model to all elements */
* { -moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box; }

body {
  background: $bodyBackgroundColor;
  background-image: $bodyBackgroundImage;

  &.drawer-visible {
    overflow: hidden;
  }

  @include retina {
    background-image: $bodyBackgroundImage2x;
    background-size: $bodyBackgroundImageSize;
  }
}

.main-content {
  max-width: 100%;
  padding: 118px 0 0;
  margin: 0 auto;
  @include transition(transform 0.2s $easeOutQuad);

  @include breakpoint(s) {
    padding-top: $gutter*2;
  }

  .showing-drawer & {
    @include transform(translateX(-260px));
  }

  .no-touch .sticky-header ~ & {
    padding-top: 180px;

    @include breakpoint(s) {
      padding-top: $gutter*2;
    }
  }

  .no-touch .navigation-below-header .sticky-header ~ & {
    padding-top: 180px;

    @include breakpoint(s) {
      padding-top: $gutter*2;
    }
  }
}

.page-title {
  margin: 0 0 $gutter*3;
  text-align: center;

  a {
    color: $headingColor;

    &:hover {
      color: $headingColor;
    }
  }
}

.rte .tabs {
  display: block;
  width: 100%;
  height: 42px;
  border-bottom: 1px solid $borderColor;
  list-style: none;
  padding: 0;
  margin: 30px 0 0;
  white-space: nowrap;

  li {
    display: inline-block;
    height: 41px;
    font-size: 14px;
    line-height: 42px;
