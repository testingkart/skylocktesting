    padding-top: $gutter*2;
    border-top: 1px solid $borderColor;

    @include breakpoint(s) {
      margin-top: $gutter;
      padding-top: $gutter;
    }
  }
}

.template-cart {

  .page-title {
    @include breakpoint(s) {
      margin-bottom: $gutter*2;
    }
  }

  .empty {
    text-align: center;
  }
}

.cart-items {
  width: 100%;

  @include transition(height 0.3s ease-in);

  &.empty {
    height: 0;
    overflow: hidden;
  }

  th {
    font-weight: normal;
    color: $bodyColor;
    text-align: right;
  }

  tr td, tr th {
    border-bottom: 1px solid $borderColor;
    border-radius: 0 !important; // override regular table styles

    &.first {
      padding-left: 0;
    }

    &.last {
      padding-right: 0;
      text-align: right;
    }
  }
}

.cart-items {
  @include breakpoint(s) {
    tr, td {
      display: block;
      width: 100%;
      float:left\9;
    }

    tr {
      border-bottom: 1px solid $borderColor;
      overflow: hidden; // unable to user clearfix class

      &.first {
        border-top: 1px solid $borderColor;
      }
    }

    tr td {
      border: none;

      &.last {
        text-align: center;
        padding-right: $gutter/3;
      }
    }

    thead, .total {
      display: none;
    }
  }
}

.cart-item {

  td {
    padding: 40px 0;
  }

  @include transition(opacity 0.3s ease-in, height 0.3s ease-in);

  &.removing {
    opacity: 0;
  }
}

.cart-item-product {
  text-align: left;

  @include breakpoint(s) {
    text-align: center;
    border-bottom: 1px solid $borderColor !important;
  }
}

.cart-image {
  display: inline-block;
  vertical-align: middle;

  img {
    display: block;
    margin: 0 auto;
  }

  @include breakpoint(s) {
    display: block;
    width: 100px;
    margin: 0 auto $gutter/2;
  }
}

.cart-item-product-wrap {
  display: inline-block;
  margin-left: $gutter;
  vertical-align: middle;

  span {
    display: block;
  }

  @include breakpoint(s) {
    display: block;
    width: 100%;
    margin-left: 0;
    text-align: center;
  }
}

.cart-title {
  font-family: $headingFont;
  -webkit-font-smoothing: initial;

  -webkit-font-smoothing: #{'{% if settings.heading-font == "Bitter, serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.heading-font == "Roboto, sans-serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.heading-font == "\'Open Sans\', sans-serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.heading-font == "Volkhov, serif" %}'} antialiased #{'{% endif %}'};
  -webkit-font-smoothing: #{'{% if settings.body-font == "Lato, sans-serif" %}'} antialiased #{'{% endif %}'};

  a {
    color: $headingColor;
  }
}

.cart-vendor, .cart-variant {
  color: $metaColor;
  font-size: 14px;
  margin-top: $gutter/3;
}

.cart-vendor ~ .cart-variant {
  margin-top: 0;
}

.cart-item-remove {
  color: $accentColor;
  font-size: 13px;
  margin-top: 10px;
  cursor: pointer;

  &:hover {
    color: $accentColorHover;
  }
}

tr {
  .cart-item-price, .cart-item-quantity, .cart-item-total {
    text-align: right;
    padding-right: 20px;

    @include breakpoint(s) {
      width: 100%;
      padding: 0 10px;
      margin: 20px 0;
      text-align: center;
    }
  }
}

.cart-item-price:before, .cart-item-quantity:before, .cart-item-total:before {
  @include breakpoint(s) {
    color: $headingColor;
  }
}

.cart-item-price:before {
  @include breakpoint(s) {
    content: 'Price: ';
  }
}

.cart-item-quantity:before {
  @include breakpoint(s) {
    content: 'Quantity: ';
  }
}

.cart-item-total:before {
  @include breakpoint(s) {
    content: 'Total: ';
  }
}

.cart-item-quantity {
  font-size: 0;
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none;

  > * {
    font-size: 16px;
  }
}

input.cart-item-quantity-display {
  display: inline-block;
  margin-right: 10px;
  width: 40px;
  height: auto;
  padding: 0;
  border: none;
  font-weight: 700;
  text-align: right;

  @include breakpoint(s) {
    margin-right: 5px;
    width: 30px;
    text-align: left;
  }
}

.cart-item-decrease, .cart-item-increase {
  display: inline-block;
  height: 36px;
  line-height: 32px;
  border: 1px solid $borderColor;
  padding: 0 10px;
  cursor: pointer;
}

.cart-item-decrease {
  border-radius: 5px 0 0 5px;
}

.cart-item-increase {
  border-left: none;
  border-radius: 0 5px 5px 0;
}

.cart-tools {
  @extend .clearfix;
  font-size: 0;
}

.cart-instructions, .cart-totals {
  display: inline-block;
  vertical-align: top;
  font-size: 16px;
  margin-top: $gutter;
}

.cart-instructions {
  width: 60%;

  @include breakpoint(s) {
    width: 100%;
  }

  textarea {
    font-size: 14px;
  }
}

.cart-totals {
  float: right;
  text-align: right;
  width: 40%;
  padding-left: $gutter;

  @include breakpoint(s) {
    width: 100%;
    padding-left: 0;
  }
}

.cart-additional-checkout-buttons {
  @extend .clearfix;
  margin-top: 25px;

  input {
    display: block;
    float: right;
    margin-bottom: 10px;
  }
}

.cart-price {
  font-size: 20px;
  color: $headingColor;
}

.cart-checkout {
  margin-top: $gutter;
}

.cart-currency-note {
  margin-top: $gutter;
}

.cart-modal-wrapper {
  @extend %modal-wrapper;

  .cart-modal { @extend %modal; }
  .cart-modal .section-title { @extend %modal-title; }
  .cart-modal-action { @extend %modal-action-button; }
}

.cart-item-undo {
  @include breakpoint(s) {
    text-align: center;
  }
}

.cart-undo {
  cursor: pointer;
  color: $accentColor;

  &:hover {
    color: $accentColorHover;
  }
}

.cart-shipping-calculator-wrapper {
  font-size: 0;
  margin-top: $gutter*2;
}

.cart-shipping-calculator {
  width: 60%;
  display: inline-block;
  vertical-align: top;
  font-size: 16px;

  @include breakpoint(s) {
    width: 100%;
  }
}

.cart-shipping-calculator-response {
  width: 40%;
  display: inline-block;
  vertical-align: top;
  font-size: 14px;
  padding: 39px 0 0 $gutter;
  text-align: right;

  @include breakpoint(s) {
    width: 100%;
    text-align: center;
    padding: 0;
  }
}

.shipping-country-wrapper, .shipping-province-wrapper {
