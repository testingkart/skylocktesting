    }
  }

  .rte {
    font-size: 20px;
    margin: $gutter auto 0;
    width: 60%;
    font-family: proxima_novalight;
    @include breakpoint(s) {
      font-size: 18px;
    }
  }

  img {
    display: block;
    margin: 0 auto;
    max-width: 100%;
  }

  img, .home-full-width-video {
    margin-top: 95px;
  }

  > *:first-child {
    margin-top: 0;
  }
}

.home-full-width-cta {
  margin-top: $gutter*2;
  text-align: center;
}

.home-left-right-feature {
