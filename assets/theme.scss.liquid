.product-list-item-thumbnail {
  position: relative;
  display: inline-block;
  max-width: 100%;

  img {
    display: block;
    margin: 0 auto;
  }
}

.stock-overlay {
  position: absolute;
  top: 50%;
  left: 50%;
  background: $accentColor;
  color: $primaryButtonColor;
  display: inline-block;
  padding: 10px 15px;
  @include transform(translate(-50%, -50%));
}

.sale-badge {
  position: absolute;
  bottom: -($gutter/3);
  right: -($gutter/3);
  background: $accentColor;
  color: $primaryButtonColor;
  display: block;
  border-radius: 50%;
  height: 30px;
  width: 30px;
  padding: 7px 7px 6px 6px;

  @include iconFont();
}

.product-list-item-vendor {
