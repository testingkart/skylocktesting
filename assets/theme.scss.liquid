  &.products-per-row-3, &.products-per-row-4 {
    margin-left: -$gutter/2;
    margin-right: -$gutter/2;
  }

  @include breakpoint(m) {
    margin-left: -$gutter/2;
    margin-right: -$gutter/2;
  }

  @include breakpoint(s) {
    margin: $gutter*3 0;
  }

  .collection-has-featured-image & {
    max-width: $maxWidth;
    margin-left: auto;
    margin-right: auto;

    &.products-per-row-3, &.products-per-row-4 {
      max-width: $maxWidth;
    }

    @include breakpoint(m) {
      padding: 0 $gutter/2;
    }

    @include breakpoint(s) {
      padding: 0 $gutter;
    }
  }
}

.product-list-item {
  font-size: 16px;
  display: inline-block;
  vertical-align: top;
  width: 50%;
  text-align: center;
  margin: 0 0 $gutter*2;
  padding: 0 $gutter;

  .products-per-row-3 & {
    width: 33.33%;
    padding: 0 $gutter/2;
  }

  .products-per-row-4 & {
    width: 25%;
    padding: 0 $gutter/2;
  }

  @include breakpoint(m) {
    padding: 0 $gutter/2;
  }

  @include breakpoint(s) {
    padding: 0!important;
    display: block;
    width: 100% !important;
  }

  h1 {
    font-size: 16px;

    a {
      color: $headingColor;
    }
  }
