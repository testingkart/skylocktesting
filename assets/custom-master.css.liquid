.home-top-section p {
  font-family: proxima_novalight;
  font-size: 18px;
  max-width: 500px;
  width: 100%;
}

.home-top-section p.text-note {
  font-size: 13px;
  margin: 10px 0 !important;
  max-width: 100%;
}

.home-top-section h2 {
  font-size: 35px;
  font-weight: normal;
  margin: 0 0 50px;
}

.home-top-section span.price-par {
  color: #4a4a4a;
  display: block;
  font-size: 28px;
  margin: 45px 0;
}

.home-top-section span.price-par strike{
  margin-right: 15px;
}

.home-top-section a {

}

	.button.preorder.clicked-preorder.slider-prebutton {
    	font-size: 15px;
    	font-weight: normal;
    	height: 42px;
    	line-height: 42px;
    	width: 220px;
	}


.home-top-section-wrapper{

}


.home-top-section .column-2 {
  display: table-cell;
  float: none!important;
  vertical-align: middle;
  width: 50%;
}

.home-top-section{
  display: table;
  margin: 0 auto;
  max-width: 1200px;
  padding: 0 50px 50px;
  width: 100%;
}

.home-brands .home-full-width-feature.home-module {
  display: table;
  margin: 0 auto;
  max-width: 1453px;
  padding: 85px 50px 85px;
  width: 100%;
}

.img-small-show{
  display: none; 
}


.home-top-section .imgpart img {
  float: right;
  max-width: 400px;
  width: 100%;
}

.main-header-wrapper {
  height: auto !important;
  padding-bottom: 20px !important;
  padding-top: 20px !important;
}

.home-brands .brands .brands-ul {
  display: table;
  list-style: outside none none;
  margin: 0;
  min-height: 80px;
  text-align: left;
  width: 100%;
}

.home-brands .brands .brands-ul .brand-li {
  display: table-cell;
  margin: 0;
  min-height: 80px;
  vertical-align: top;
  width: 14.2857%;
}

.home-brands .brands .brands-ul .brand-li a {
  display: table;
  float: right;
  text-align: right;
}



.home-top-section .button.videobutton{
  margin-left: 30px;
  background-color: transparent;
  border: 1px solid #8a8a8a;
  border-radius: 23px;
  color: #8a8a8a;
  font-size: 15px;
  font-weight: normal;
  height: 42px;
  text-align: center;
  width: 220px;
  letter-spacing: 0;
  display: inline-block !important;
  vertical-align: middle !important;
  line-height: 40px;
  padding: 0!important;
}

.home-top-section .button.videobutton span {
  display: inline-block !important;
  float: none;
  line-height: 40px;
  margin: 0 5px;
  text-align: center !important;
  vertical-align: middle !important;
}
span.videobutton-icon{
  background-image: url({{ "play-black.png" | asset_url }});
  background-color: transparent;
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 100% auto;
  display: inline-block;
  height: 20px;
  text-align: center;
  vertical-align: middle;
  width: 20px;
}

span.na-shipping {
    display: inline-block;
    font-size: 18px;
    margin: 0 0 0 15px;
    vertical-align: middle;
}

@media screen and (min-width: 1200px) {

}

@media only screen and (orientation: portrait) and (device-width: 320px), (device-width: 768px) {

}
@media only screen and (orientation: landscape) and (device-width: 320px), (device-width: 768px) {

}


@media screen and (max-width: 1200px){
  .home-top-section a {
    width: 195px !important;
  }

  .home-top-section .button.videobutton{
    padding-left: 50px;
  }
}
@media screen and (max-width: 992px){
  .home-brands .home-full-width-feature.home-module {
    max-width: 1453px;
    padding: 35px 30px;
  }

  .home-brands .brands .brands-ul .brand-li {
    width: 16%;
  }

  .home-brands .brands .brands-ul .brand-li a {
    display: inline-block;
    float: none;
    text-align: center;
  }

  .home-brands .brands .brands-ul .brand-li.featured-in-li {
    margin: 0;
    width: 100%;
  }
  .home-brands .brands .brands-ul .brand-li {
    display: inline-block;
    text-align: center;
  }


  .button.preorder.clicked-preorder.slider-prebutton{
    display: none; 
  }

  .home-top-section p.first-par{
    display: none; 
  }


  .home-top-section span.price-par {
    display: none !important;
  }


  .home-top-section .button.videobutton {
    margin: 20px 0 40px;
    padding-left: 50px;
    width: 220px !important;
  }

  .home-top-section h2 {
    font-size: 30px;
    font-weight: normal;
    margin: 0 0 20px;
  }

  .home-top-section span.price-par {
    color: #4a4a4a;
    display: block;
    font-size: 28px;
    margin: 20px 0;
  }

  .home-top-section p {
    margin: 0 0 10px !important;
    max-width: 500px;
    width: 100%;
  }

  .home-top-section p.text-note {
    font-size: 13px;
    margin: 10px 0;
    max-width: 100%;
  }

  .home-top-section {
    display: table;
    margin: 0 auto;
    max-width: 1453px;
    padding: 25px;
    width: 100%;
  }
}

@media screen and (max-width: 797px){
  .home-top-section h2 {
    font-size: 25px !important;
    font-weight: normal;
    margin: 0 0 10px !important;
  }

  .home-top-section span.price-par {
    color: #4a4a4a;
    display: block;
    font-size: 22px;
    margin: 10px 0;
  }

  .home-top-section p.text-note {
    font-size: 13px;
    margin: 10px 0 !important;
    max-width: 100%;
    line-height: 20px;
  }

  .home-top-section .column-2.contentpart {
    float: left;
    padding: 0 15px 0 0;
    width: 60%;
  }

  .home-top-section .column-2.imgpart {
    float: left;
    padding: 0 0 0 15px;
    width: 40%;
  }

  .home-top-section .button.videobutton {
    margin: 15px 0;
    padding-left: 50px;
    width: 220px !important;
  }

  .home-top-section p {
    font-size: 14px;
    line-height: 22px;
    margin: 0 0 10px !important;
    max-width: 500px;
    width: 100%;
  }
}

@media screen and (max-width: 639px){
  .home-top-section .button.videobutton {
    height: 35px;
    line-height: 33px;
    margin: 10px 0;
    width: 200px !important;
  }

  .home-top-section .button.videobutton span{
    line-height: 33px; 
  }

  span.videobutton-icon{
    height: 19px;
    width: 19px;
  }

  .home-top-section p {
    font-size: 13px !important;
    line-height: 20px !important;
    margin: 0 0 10px !important;
    max-width: 100%;
    width: 100%;
  }

  .home-top-section p.text-note {
    font-size: 11px !important;
    margin: 5px 0 20px !important;
    max-width: 100%;
  }

  .home-top-section h2 {
    font-size: 20px !important;
    font-weight: normal;
    margin: 0 0 10px !important;
  }

  .home-brands .brands .brands-ul .brand-li {
    width: 32%;
  }
  .home-brands .brands .brands-ul .brand-li img {
    height: 50px;
    margin: 0 auto;
    width: auto;
  }

}

@media screen and (max-width: 520px){

  .template-index .main-header-wrapper {
    background: none;
  }

  .template-index .main-content {
    padding-top: 0;
  }

}

@media screen and (max-width: 479px){
  .home-top-section h2 {
    font-size: 14px !important;
    font-weight: 700;
  }
  .home-top-section .button.videobutton {
    background-position: 15px center;
    font-size: 11px;
    height: 30px;
    line-height: 30px;
    margin: 5px 0 10px;
    width: 130px !important;
  }
  .home-top-section .column-2.contentpart{
    padding-right: 10px; 
  }

  .home-top-section .column-2.imgpart{
    padding-left: 10px; 
  }

  .home-top-section {
    padding: 15px 15px 15px;
    width: 100%;
  }

  .home-brands .brands .brands-ul .brand-li img {
    height: 50px;
    margin: 0 auto;
    width: auto;
  }

  .home-brands .home-full-width-feature.home-module {
    max-width: 1453px;
    padding: 25px 15px;
  }

  .home-top-section .button.videobutton{
    line-height: 29px; 
  }

  .home-top-section .button.videobutton span {
    line-height: 29px;
  }


}
